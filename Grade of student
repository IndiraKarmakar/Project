#include<stdio.h>
void main()
{
    int mark;

    printf("Enter your marks: \n");
    scanf("%d", &mark);

    if (mark>=85)
    {
        printf("Grade A\n");
    }
    else if (mark>=70 && mark<=84)
    {
        printf("Grade B\n");
    }
    else if (mark>=55 && mark<=69)
    {
        printf("Grade C\n");
    }
    else if (mark>=40 && mark<=54)
    {
        printf("Grade D\n");
    }
    else
    {
        printf("Grade F\n");
    }
    return 0;
}
